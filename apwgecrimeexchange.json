    "configuration": {
        "Authorization Key": {
            "description": "API Key for eCrimeX",
            "data_type": "password",
            "required": true,
            "order": 0,
            "name": "Authorization Key",
            "id": 0
        }
    },
    "actions": [
        {
            "action": "test connectivity",
            "identifier": "test_connectivity",
            "description": "Validate the asset configuration for connectivity using supplied configuration",
            "type": "test",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "url reputation",
            "identifier": "url_reputation",
            "description": "Queries URL info",
            "type": "investigate",
            "read_only": true,
            "parameters": {
                "url": {
                    "description": "URL to query",
                    "data_type": "string",
                    "required": true,
                    "order": 0,
                    "name": "url"
                },
                "filter": {
                    "description": "Phish endpoint to query",
                    "data_type": "string",
                    "required": true,
                    "value_list": [
                        "url_exact",
                        "domain",
                        "url"
                    ],
                    "order": 1,
                    "name": "filter"
                },
                "sanitize_url": {
                    "description": "Sanitizes provided URL",
                    "data_type": "boolean",
                    "order": 2
                }
            },
            "output": [
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.parameter.filter",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.parameter.sanitize_url",
                    "data_type": "boolean"
                },
                {
                    "data_path": "action_result.parameter.url",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.data.*._embedded.phish.*.confidence_level",
                    "data_type": "numeric",
                    "column_name": "Confidence Level",
                    "column_order": 2
                },
                {
                    "data_path": "action_result.data.*._embedded.phish.*.status",
                    "data_type": "numeric",
                    "column_name": "Status",
                    "column_order": 3
                },
                {
                    "data_path": "action_result.data.*._embedded.phish.*.url",
                    "data_type": "string",
                    "contains": [
                        "url"
                    ],
                    "column_name": "URL",
                    "column_order": 0
                },
                {
                    "data_path": "action_result.data.*.total_found",
                    "data_type": "numeric",
                    "column_name": "Entry Found",
                    "column_order": 1
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric"
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric"
                }
            ],
            "render": {
                "type": "table"
            },
            "versions": "EQ(*)"
        }
    ],
    "pip_dependencies": {
        "wheel": [
